{
  "name": "create-gatsby-electron-app",
  "version": "0.1.0",
  "description": "A simple starter project to get up and developing quickly with the blend of Gatsby and Electron JS for building a Native Desktop App on various OS Platforms like Windows, Linux and macOS using Electron Builder.",
  "main": "main.js",
  "author": "Souleh Shaikh <author.souleh@thegeekytutor.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/soulehshaikh99/create-gatsby-electron-app"
  },
  "bugs": {
    "url": "https://github.com/soulehshaikh99/create-gatsby-electron-app/issues"
  },
  "build": {
    "icon": "src/images/gatsby-icon.png",
    "productName": "Gatsby and Electron App",
    "files": [
      "public/**/*",
      "main.js"
    ]
  },
  "scripts": {
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "prebuild": "yarn run clean",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "electron": "wait-on http://localhost:8000 && electron .",
    "electron-dev": "concurrently \"yarn run start\" \"yarn run electron\"",
    "preelectron-pack": "yarn run build",
    "electron-pack": "electron-builder"
  },
  "dependencies": {
    "electron-serve": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^9.0.0",
    "electron-builder": "^22.6.1",
    "gatsby": "^2.22.11",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-offline": "^3.2.7",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sharp": "^2.6.9",
    "gatsby-source-filesystem": "^2.3.8",
    "gatsby-transformer-sharp": "^2.5.3",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "wait-on": "^5.0.0"
  },
  "keywords": [
    "application",
    "blog",
    "concurrently",
    "cross-platform",
    "css",
    "desktop-app",
    "electron-builder",
    "electron-serve",
    "electron",
    "framework",
    "gatsby-cli",
    "gatsby-starter",
    "gatsby",
    "gatsbyjs",
    "graphql",
    "html",
    "installed-app",
    "javascript-framework",
    "javascript",
    "linux",
    "macos",
    "native-app",
    "npm",
    "react",
    "responsive-app",
    "static-site-generator",
    "wait-on",
    "web-framework",
    "windows",
    "yarn"
  ]
}
